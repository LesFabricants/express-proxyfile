{
  "name": "@lesfabricants/express-proxyfile",
  "version": "1.0.1",
  "description": "A simple library to replace any current express application that uses the file system to serve files to switch to a cloud service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "rm -rf dist && barrelsby --delete -d lib",
    "build": "tsc -p tsconfig.json"
  },
  "author": "Julien Donque / Les Fabricants",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:LesFabricants/express-proxyfile.git"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "barrelsby": "^2.8.0",
    "typescript": "^5.1.3"
  },
  "optionalDependencies": {
    "@aws-sdk/client-s3": "^3.350.0"
  }
}
